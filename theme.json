{
    "version": 2,
    "settings": {
        "appearanceTools": true,
        "color": {
            "palette": [
                {
                    "slug": "bg",
                    "name": "Background",
                    "color": "#ffffff"
                },
                {
                    "slug": "text",
                    "name": "Text",
                    "color": "#111111"
                },
                {
                    "slug": "primary",
                    "name": "Primary",
                    "color": "#0a84ff"
                },
                {
                    "slug": "secondary",
                    "name": "Secondary",
                    "color": "#5856d6"
                },
                {
                    "slug": "muted",
                    "name": "Muted",
                    "color": "#f3f4f6"
                }
            ]
        },
        "typography": {
            "fluid": true,
            "fontFamilies": [
                {
                    "slug": "system-sans",
                    "name": "System Sans",
                    "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Ubuntu, 'Helvetica Neue', Arial, 'Noto Sans', 'Liberation Sans', sans-serif"
                }
            ],
            "fontSizes": [
                {
                    "slug": "sm",
                    "name": "Small",
                    "size": "clamp(0.875rem, 0.8rem + 0.2vw, 0.95rem)"
                },
                {
                    "slug": "base",
                    "name": "Base",
                    "size": "clamp(1rem, 0.9rem + 0.3vw, 1.125rem)"
                },
                {
                    "slug": "lg",
                    "name": "Large",
                    "size": "clamp(1.125rem, 1rem + 0.5vw, 1.25rem)"
                },
                {
                    "slug": "xl",
                    "name": "XL",
                    "size": "clamp(1.25rem, 1.1rem + 0.8vw, 1.5rem)"
                },
                {
                    "slug": "xxl",
                    "name": "2XL",
                    "size": "clamp(1.5rem, 1.3rem + 1.2vw, 2rem)"
                }
            ],
            "lineHeight": "1.6"
        },
        "spacing": {
            "units": [
                "px",
                "rem",
                "%",
                "vw",
                "vh"
            ],
            "blockGap": "var:preset|spacing|40",
            "spacingSizes": [
                {
                    "slug": "16",
                    "name": "8px",
                    "size": "0.5rem"
                },
                {
                    "slug": "24",
                    "name": "12px",
                    "size": "0.75rem"
                },
                {
                    "slug": "32",
                    "name": "16px",
                    "size": "1rem"
                },
                {
                    "slug": "40",
                    "name": "24px",
                    "size": "1.5rem"
                },
                {
                    "slug": "64",
                    "name": "32px",
                    "size": "2rem"
                }
            ]
        },
        "layout": {
            "contentSize": "720px",
            "wideSize": "1100px"
        }
    },
    "styles": {
        "color": {
            "background": "var(--wp--preset--color--bg)",
            "text": "var(--wp--preset--color--text)"
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--system-sans)",
            "fontSize": "var(--wp--preset--font-size--base)"
        },
        "elements": {
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--primary)"
                }
            },
            "heading": {
                "typography": {
                    "lineHeight": "1.2"
                },
                "color": {
                    "text": "var(--wp--preset--color--text)"
                }
            }
        },
        "blocks": {
            "core/navigation": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--base)"
                }
            },
            "core/button": {
                "border": {
                    "radius": "9999px"
                },
                "spacing": {
                    "padding": {
                        "top": "0.6em",
                        "right": "1.1em",
                        "bottom": "0.6em",
                        "left": "1.1em"
                    }
                }
            }
        }
    }
}